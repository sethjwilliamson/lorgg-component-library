{
  "name": "vue3-comp-lib-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --ignore-path cardJson.json --ignore-path data.json --fix src",
